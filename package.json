{
  "name": "homebank-apostle",
  "version": "1.0.0",
  "description": "Converts exported transaction statements to Homebank format for import.",
  "main": "./lib/index/js",
  "bin": {
    "hba": "./lib/index.js"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc -p .",
    "local": "sudo npm i -g && hba",
    "create": "npm run build && npm run test",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'DEBUG=* ts-node' src/index.ts",
    "start:prod": "nodemon --watch 'src/**/*.ts' --exec 'DEBUG=* ts-node' src/index.ts",
    "run": "ts-node src/index.ts",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimeBandit/homebank-apostle.git"
  },
  "keywords": [
    "homebank",
    "banking",
    "csv",
    "transactions"
  ],
  "author": "Imran Nazir (imran.nazir@yahoo.co.uk)",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/TimeBandit/homebank-apostle/issues"
  },
  "homepage": "https://github.com/TimeBandit/homebank-apostle#readme",
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "@types/jest": "^26.0.20",
    "@types/line-reader": "^0.0.33",
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^14.14.25",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "mock-stdin": "^1.0.0",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@types/papaparse": "^5.2.5",
    "enquirer": "^2.3.6",
    "line-reader": "^0.4.0",
    "logging": "^3.3.0",
    "mock-fs": "^4.13.0",
    "papaparse": "^5.3.0"
  }
}
